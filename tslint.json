{
    "extends": "dtslint/dtslint.json",
    "rules": {
        "indent": false,
        "no-redundant-jsdoc": false,
        "no-single-declare-module": false,
        "no-trailing-whitespace": false,
        "no-unnecessary-class": false,
        "semicolon": false,
        "strict-export-declare-modifiers": false
    }
}
